{
  "name": "Recipe Filter Redux",
  "author": "Michael Faith",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "A tool to skip straight to the recipe on blog based sites",
  "icons": {
    "128": "assets/icons/icon-128.png"
  },
  "default_locale": "en",
  "permissions": ["tabs", "storage"],
  "host_permissions": ["http://*/*", "https://*/*"],
  "action": {
    "default_icon": {
      "128": "assets/icons/icon-128.png"
    },
    "default_title": "Extension Boilerplate"
  },
  "web_accessible_resources": [
    {
      "resources": [],
      "matches": ["<all_urls>"]
    }
  ]
}
